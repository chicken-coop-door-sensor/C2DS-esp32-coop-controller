{
    "editor.formatOnSave": true,
    "clang-format.style": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 120 }",
    "idf.adapterTargetName": "esp32",
    "files.associations": {
        "mqtt.h": "c",
        "wifi.h": "c",
        "gecl-time-sync-manager.h": "c",
        "cjson.h": "c"
    },
    "sonarlint.pathToCompileCommands": "${workspaceFolder}/build/compile_commands.json",
    "terminal.integrated.profiles.osx": {
        "MyCustomProfile": {
            "path": "/bin/zsh",
            "args": [
                "-c",
                "source ~/.zshrc && setidf && export COMPONENT_KCONFIGS_DIR=$(pwd)/components && export EXTRA_COMPONENT_DIRS=$(pwd)/components && clear && exec zsh"
            ]
        }
    },
    "terminal.integrated.defaultProfile.osx": "MyCustomProfile",
    "workbench.startupEditor": "none",
    "workbench.welcomePage.enabled": false,
    "git.defaultCloneDirectory": "/Users/tennis/src"
}